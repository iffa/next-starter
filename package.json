{
  "name": "next-starter",
  "description": "Opinionated Next.js project starter template, with TypeScript, Chakra UI and more.",
  "version": "1.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iffa/next-starter.git"
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap",
    "start": "next start",
    "test": "start-test 'yarn dev' 3000 'yarn cy:run'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "jest": "jest",
    "jest:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write --ignore-unknown .",
    "analyze": "cross-env ANALYZE=true yarn build",
    "prepare": "husky install",
    "find-unused": "next-unused"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "**/*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@tailwindcss/aspect-ratio": "0.2.0",
    "@tailwindcss/forms": "0.3.2",
    "@tailwindcss/line-clamp": "0.2.0",
    "got": "11.8.2",
    "next": "10.2.0",
    "next-i18next": "8.2.0",
    "next-seo": "4.24.0",
    "next-yup": "1.1.0",
    "preact": "10.5.13",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@next/bundle-analyzer": "10.2.0",
    "@types/node": "14.14.37",
    "@types/react": "17.0.5",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "autoprefixer": "10.2.5",
    "cross-env": "7.0.3",
    "cypress": "7.3.0",
    "eslint": "7.26.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "7.23.2",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "next-sitemap": "1.6.57",
    "next-unused": "0.0.6",
    "postcss": "8.2.15",
    "prettier": "2.3.0",
    "start-server-and-test": "1.12.1",
    "tailwindcss": "2.1.2",
    "typescript": "4.3.4"
  }
}
